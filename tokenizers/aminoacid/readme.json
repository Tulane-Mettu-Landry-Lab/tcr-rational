{
  "name": "Amino Acid Tokenizer",
  "desc": "Tokenize amino-acid sequence bit by bit.",
  "dataset": {
    "name": "TCRR",
    "desc": "The TCRR dataset with only positive data.",
    "columns": [
      "TRA",
      "TRB",
      "epitope"
    ],
    "size": 20887,
    "remove_na": true,
    "unique": true

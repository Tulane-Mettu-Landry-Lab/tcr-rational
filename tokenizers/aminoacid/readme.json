      "pad_token": "<PAD>",
      "unk_token": "<MIS>",
      "sep_token": "<SEP>",
      "bos_token": "<BOS>"
    },
    "split_pattern": "",
    "split_behavior": "isolated"
  },
  "tokenizer": {
    "vocab_size": 28,
    "special_tokens": {
      "eos_token": "<EOS>",
      "unk_token": "<MIS>",
      "sep_token": "<SEP>",
      "pad_token": "<PAD>",
      "cls_token": "<CLS>",
      "mask_token": "<MSK>"
    }
  }

{
  "name": "TRBV Component Tokenizer",
  "desc": "Tokenize TRBV alleles following components.",
  "dataset": {
    "name": "TCRR",
    "desc": "The TCRR dataset with only positive data.",
    "columns": [
      "TRBV"
    ],
    "size": 90,
    "remove_na": true,
    "unique": true
  },
  "builder": {
    "vocab_size": 1000,
    "model": "WordLevel",
    "special_tokens": {
      "eos_token": "<EOS>",
      "mask_token": "<MSK>",
      "cls_token": "<CLS>",
      "pad_token": "<PAD>",
      "unk_token": "<MIS>",
      "sep_token": "<SEP>",
      "bos_token": "<BOS>"
    },
    "split_pattern": "[*:/-]",
    "split_behavior": "removed"
  },
  "tokenizer": {
    "vocab_size": 52,
    "special_tokens": {
      "eos_token": "<EOS>",
      "unk_token": "<MIS>",
      "sep_token": "<SEP>",
      "pad_token": "<PAD>",
      "cls_token": "<CLS>",
      "mask_token": "<MSK>"
    }
  }
}